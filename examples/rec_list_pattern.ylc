let printf = fn (fmt: str, ...): int

let func = fn (i) { i + 1 }

let arr = [
	0, 
	1, 
	2, 
	3, 
	4, 
	5, 
	6, 
	7, 
	8, 
]
let iter_list = fn (l): int {
	match l
	| [] -> 1
	| x if x > 2 -> 2
	| [x, ..rest] -> {
		printf("%d\n", x)
		iter_list(rest)
	}
	| [1, ..rest] -> {
		printf("found %d\n", 1)
		1
	}
}

iter_list(arr)
